{
  "name": "apicypress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cy:run": "cypress run",
    "test_cypress": "cypress run --spec \"**/*.feature\"",
    "test_cypress:regressivo": "npx cypress run --env grepTags='@regressivo'",
    "pretest": "rm -rf cypress/reports",
    "test": "cypress run",
    "posttest": "node cypress/reports/cucumber-html-report.js"
  },
  "devDependencies": {
    "cucumber": "^6.0.0",
    "cucumber-html-reporter": "^5.5.1",
    "cypress": "^14.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-grep": "^3.0.4",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-tags": "^1.2.2",
    "typescript": "^5.7.3"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".json"
    }
  }
}
